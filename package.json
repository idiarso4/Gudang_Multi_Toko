{
  "name": "marketplace-integration",
  "version": "1.0.0",
  "description": "Aplikasi integrasi marketplace untuk mengelola multi-toko online",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:generate": "cd server && npx prisma generate",
    "db:studio": "cd server && npx prisma studio"
  },
  "keywords": [
    "marketplace",
    "integration",
    "e-commerce",
    "multi-channel",
    "shopee",
    "tokopedia",
    "lazada"
  ],
  "author": "Marketplace Integration Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "@prisma/client": "^5.7.1",
    "redis": "^4.6.11",
    "bull": "^4.12.2",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "prisma": "^5.7.1",
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3"
  }
}
